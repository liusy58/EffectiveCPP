int main() {
	var a,b,c:int;
	a=1;
	b=a+1;
	c=plus(a,b);
	b=minus(a,a);
	a=div(c,b);
	
	return a;
}
int plus(x:int, y:int) {
	return x+y;
}
int minus(x:int, y:int) {
	return x-y;
}
int mult(x:int, y:int) {
	var sum,i:int;
	sum=0;
	for(i=0;i<x;i+=1) {
		sum+=y;
	}
	return sum;
}
int div(x:int, y:int) {
	return x/y;
}