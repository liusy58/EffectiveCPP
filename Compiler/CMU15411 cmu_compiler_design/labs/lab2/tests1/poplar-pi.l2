#test return 31312
// compute some pi by Taylor expansion of 4 arctan 1 (Gregory-Leibniz)
{
        x = 0;
        for (i = 0; i < 100; i += 1)
                if (i % 2 == 0)
                        x += 40000 / (2*i + 1);
                else
                        x -= 40000 / (2*i + 1);
        return x;
}

