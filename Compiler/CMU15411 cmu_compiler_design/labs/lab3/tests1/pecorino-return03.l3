/* Calculates the sum of all numbers from 
   n1 to n2 */
	 
int sum (x :int ) {
	var i, sum: int;

	sum = 0;
	for (i = 0; i<= x; i = i + 1)
		sum = sum + i;
	return sum;
}

int main () {
	var n1, n2, diff: int ;
	n1 = 10;
	n2 = 20;
	n1 = sum (n1);
	n2 = sum (n2);
	diff = n2 - n1 ;
	return diff ;
}

